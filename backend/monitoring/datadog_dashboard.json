{
  "title": "ProtoThrive - Thermonuclear Performance Dashboard",
  "description": "Comprehensive monitoring of ProtoThrive optimization features and performance metrics",
  "layout_type": "ordered",
  "template_variables": [
    {
      "name": "environment",
      "default": "production",
      "prefix": "environment"
    },
    {
      "name": "region",
      "default": "*",
      "prefix": "region"
    }
  ],
  "widgets": [
    {
      "id": 1,
      "definition": {
        "title": "ðŸš€ Response Time Performance",
        "title_size": "16",
        "title_align": "left",
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:protothrive.performance.response_time.p50{$environment,$region} by {region}",
            "display_type": "line",
            "style": {
              "palette": "cool",
              "line_type": "solid",
              "line_width": "normal"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.performance.response_time.p50{$environment,$region} by {region}",
                "alias_name": "P50 Response Time"
              }
            ]
          },
          {
            "q": "avg:protothrive.performance.response_time.p99{$environment,$region} by {region}",
            "display_type": "line",
            "style": {
              "palette": "warm",
              "line_type": "solid",
              "line_width": "normal"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.performance.response_time.p99{$environment,$region} by {region}",
                "alias_name": "P99 Response Time"
              }
            ]
          }
        ],
        "yaxis": {
          "label": "Response Time (ms)",
          "scale": "linear",
          "min": "auto",
          "max": "auto",
          "include_zero": true
        },
        "markers": [
          {
            "value": "y = 25",
            "display_type": "error dashed",
            "label": "P50 Target (25ms)"
          },
          {
            "value": "y = 100",
            "display_type": "warning dashed",
            "label": "P99 Target (100ms)"
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 6,
        "height": 3
      }
    },
    {
      "id": 2,
      "definition": {
        "title": "âš¡ Cache Performance by Tier",
        "title_size": "16",
        "title_align": "left",
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:protothrive.cache.hot.hit_rate{$environment,$region}",
            "display_type": "area",
            "style": {
              "palette": "green",
              "line_type": "solid",
              "line_width": "normal"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.cache.hot.hit_rate{$environment,$region}",
                "alias_name": "HOT Tier"
              }
            ]
          },
          {
            "q": "avg:protothrive.cache.warm.hit_rate{$environment,$region}",
            "display_type": "area",
            "style": {
              "palette": "blue",
              "line_type": "solid",
              "line_width": "normal"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.cache.warm.hit_rate{$environment,$region}",
                "alias_name": "WARM Tier"
              }
            ]
          },
          {
            "q": "avg:protothrive.cache.cold.hit_rate{$environment,$region}",
            "display_type": "area",
            "style": {
              "palette": "purple",
              "line_type": "solid",
              "line_width": "normal"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.cache.cold.hit_rate{$environment,$region}",
                "alias_name": "COLD Tier"
              }
            ]
          }
        ],
        "yaxis": {
          "label": "Hit Rate (%)",
          "scale": "linear",
          "min": "0",
          "max": "100",
          "include_zero": true
        },
        "markers": [
          {
            "value": "y = 90",
            "display_type": "ok dashed",
            "label": "Target Hit Rate (90%)"
          }
        ]
      },
      "layout": {
        "x": 6,
        "y": 0,
        "width": 6,
        "height": 3
      }
    },
    {
      "id": 3,
      "definition": {
        "title": "ðŸŒ Geographic Performance Distribution",
        "title_size": "16",
        "title_align": "left",
        "type": "geomap",
        "requests": [
          {
            "q": "avg:protothrive.geographic.response_time{$environment} by {region}",
            "style": {
              "palette": "hostmap_blues"
            }
          }
        ],
        "view": {
          "focus": "WORLD"
        }
      },
      "layout": {
        "x": 0,
        "y": 3,
        "width": 6,
        "height": 4
      }
    },
    {
      "id": 4,
      "definition": {
        "title": "ðŸ’° Cost Optimization Metrics",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "q": "avg:protothrive.cost.savings.monthly{$environment}",
            "aggregator": "avg",
            "conditional_formats": [
              {
                "comparator": ">",
                "value": 100,
                "palette": "green_on_white"
              },
              {
                "comparator": ">",
                "value": 50,
                "palette": "yellow_on_white"
              },
              {
                "comparator": "<=",
                "value": 50,
                "palette": "red_on_white"
              }
            ]
          }
        ],
        "custom_unit": "$",
        "precision": 2,
        "text_align": "center"
      },
      "layout": {
        "x": 6,
        "y": 3,
        "width": 3,
        "height": 2
      }
    },
    {
      "id": 5,
      "definition": {
        "title": "ðŸŽ¯ Optimization Score",
        "title_size": "16",
        "title_align": "left",
        "type": "gauge",
        "requests": [
          {
            "q": "avg:protothrive.optimization.score{$environment}",
            "conditional_formats": [
              {
                "comparator": ">=",
                "value": 0.9,
                "palette": "green_on_white"
              },
              {
                "comparator": ">=",
                "value": 0.7,
                "palette": "yellow_on_white"
              },
              {
                "comparator": "<",
                "value": 0.7,
                "palette": "red_on_white"
              }
            ]
          }
        ],
        "min": 0,
        "max": 1
      },
      "layout": {
        "x": 9,
        "y": 3,
        "width": 3,
        "height": 2
      }
    },
    {
      "id": 6,
      "definition": {
        "title": "ðŸ“Š Database Performance",
        "title_size": "16",
        "title_align": "left",
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:protothrive.performance.database.query_time{$environment,$region}",
            "display_type": "line",
            "style": {
              "palette": "dog_classic",
              "line_type": "solid",
              "line_width": "normal"
            }
          }
        ],
        "yaxis": {
          "label": "Query Time (ms)",
          "scale": "linear",
          "min": "auto",
          "max": "auto",
          "include_zero": true
        },
        "markers": [
          {
            "value": "y = 20",
            "display_type": "warning dashed",
            "label": "Target Query Time (20ms)"
          }
        ]
      },
      "layout": {
        "x": 6,
        "y": 5,
        "width": 6,
        "height": 2
      }
    },
    {
      "id": 7,
      "definition": {
        "title": "ðŸ’¸ Budget Utilization",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "q": "avg:protothrive.cost.budget.utilization_pct{$environment}",
            "aggregator": "last",
            "conditional_formats": [
              {
                "comparator": ">=",
                "value": 95,
                "palette": "red_on_white"
              },
              {
                "comparator": ">=",
                "value": 80,
                "palette": "yellow_on_white"
              },
              {
                "comparator": "<",
                "value": 80,
                "palette": "green_on_white"
              }
            ]
          }
        ],
        "custom_unit": "%",
        "precision": 1,
        "text_align": "center"
      },
      "layout": {
        "x": 0,
        "y": 7,
        "width": 3,
        "height": 2
      }
    },
    {
      "id": 8,
      "definition": {
        "title": "ðŸš¦ Argo Routing Performance",
        "title_size": "16",
        "title_align": "left",
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:protothrive.argo.improvement_ms{$environment}",
            "display_type": "bars",
            "style": {
              "palette": "cool"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.argo.improvement_ms{$environment}",
                "alias_name": "Avg Improvement (ms)"
              }
            ]
          },
          {
            "q": "avg:protothrive.argo.optimization_rate{$environment} * 100",
            "display_type": "line",
            "style": {
              "palette": "warm"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.argo.optimization_rate{$environment} * 100",
                "alias_name": "Optimization Rate (%)"
              }
            ]
          }
        ],
        "yaxis": {
          "label": "Performance Metrics",
          "scale": "linear",
          "min": "auto",
          "max": "auto"
        }
      },
      "layout": {
        "x": 3,
        "y": 7,
        "width": 9,
        "height": 2
      }
    },
    {
      "id": 9,
      "definition": {
        "title": "ðŸ“ˆ Request Volume & Error Rate",
        "title_size": "16",
        "title_align": "left",
        "type": "timeseries",
        "requests": [
          {
            "q": "sum:protothrive.geographic.request_count{$environment} by {region}.as_rate()",
            "display_type": "bars",
            "style": {
              "palette": "cool"
            },
            "metadata": [
              {
                "expression": "sum:protothrive.geographic.request_count{$environment} by {region}.as_rate()",
                "alias_name": "Requests/sec"
              }
            ]
          },
          {
            "q": "avg:protothrive.geographic.error_rate{$environment} by {region} * 100",
            "display_type": "line",
            "style": {
              "palette": "warm",
              "line_type": "solid",
              "line_width": "thick"
            },
            "metadata": [
              {
                "expression": "avg:protothrive.geographic.error_rate{$environment} by {region} * 100",
                "alias_name": "Error Rate (%)"
              }
            ]
          }
        ],
        "yaxis": {
          "label": "Volume & Error Rate",
          "scale": "linear",
          "min": "auto",
          "max": "auto"
        },
        "markers": [
          {
            "value": "y = 1",
            "display_type": "error dashed",
            "label": "Error Rate Threshold (1%)"
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 9,
        "width": 12,
        "height": 3
      }
    },
    {
      "id": 10,
      "definition": {
        "title": "ðŸ”¥ Thermonuclear Performance Summary",
